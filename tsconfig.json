{
  "compilerOptions": {
    //change3
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "components/*"
      ]
    },
    //change2
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    //change1
    "noEmit": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "noImplicitAny": true,
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/_app.jsx", "pages/index.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
